
<html>
  <head>
    <meta charset="UTF-8">
    <title>tokugai</title>
  </head>
  <body>
    <header>
      <div class="heading">
        <%= link_to image_tag("rogo.png", class: :logo), root_path %>
        
      </div>
      <div class="navbar">
        <ul class="nav-lists">
          <li>カテゴリからさがす</li>
          <li>キーワードからさがす</li>

          <li class="login">ログイン・新規登録</li>
      
        </ul>
      </div>
    </header>



  </html>
  <h2 class="heading-name">ログイン</h2>
  <div class="log-content">
<%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
  
    <div class="s-field">
    <%= f.email_field :email, class:"input-default", id:"email", placeholder:"メールアドレス", autofocus: true %>
  </div>

  <div>
    <%= f.password_field :password,class:"input-default", autocomplete: "current-password" , placeholder:"パスワード"%>
  </div>

   </div>
  <div>
    <%= f.submit "ログイン",class:"login-org-btn" %>
  </div>
<% end %>

<div class="rea">
<%= render "devise/shared/links" %>
</div>

<footer>
  <p>Copyright(C) tokugai ALL RIGHTS </p>
</footer>
  </body>
  